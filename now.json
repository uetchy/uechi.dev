{
  "version": 2,
  "alias": ["uechi.dev"],
  "routes": [
    {
      "src": "/twitter",
      "status": 301,
      "headers": { "Location": "https://twitter.com/uetschy" }
    },
    {
      "src": "/facebook",
      "status": 301,
      "headers": { "Location": "https://www.facebook.com/uetchy" }
    },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": { "Location": "https://github.com/uetchy" }
    }
  ]
}
